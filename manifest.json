{
    "name": "GS Activity Counter",
    "version": "1.3",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "http://hlstats.fakaheda.eu/*/hlstats.php?mode=playersessions&player=*"
            ],
            "js": ["scripts/app.js"]
        }
    ],
    "icons": {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
}